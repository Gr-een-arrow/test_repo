[
  {
    "test_case_id": "TC_001",
    "description": "Valid round-trip search with correct IATA codes and future dates",
    "test_type": "Positive",
    "test_steps": [
      "Set OriginDepCriteria.IATA_LocationCode to 'LHR'",
      "Set OriginDepCriteria.Date to '2025-05-07'",
      "Set DestArrivalCriteria.IATA_LocationCode to 'JNB'",
      "Set return OriginDepCriteria.IATA_LocationCode to 'JNB'",
      "Set return OriginDepCriteria.Date to '2025-05-14'",
      "Set return DestArrivalCriteria.IATA_LocationCode to 'LHR'",
      "Set CabinTypeCode to '5' and PrefLevelCode to 'Required'",
      "POST AirShoppingRQ"
    ],
    "expected_result": "HTTP 200 with non-empty list of available flight offers for both legs",
    "priority": "High",
    "change_note": "UNCHANGED"
  },
